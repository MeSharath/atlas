{
  "name": "soloai-travel-planner",
  "version": "1.0.0",
  "description": "AI-powered travel planner for solo travelers",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:functions\"",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:functions": "cd src/functions && npm run serve",
    "build": "npm run build:frontend && npm run build:functions",
    "build:frontend": "cd src/frontend && npm run build",
    "build:functions": "cd src/functions && npm run build",
    "test": "npm run test:frontend && npm run test:functions",
    "test:frontend": "cd src/frontend && npm test",
    "test:functions": "cd src/functions && npm test",
    "test:e2e": "cypress run",
    "deploy": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "emulator": "firebase emulators:start",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/"
  },
  "keywords": [
    "travel",
    "ai",
    "solo-travel",
    "itinerary",
    "gemini",
    "firebase",
    "maps"
  ],
  "author": "SoloAI Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "cypress": "^13.0.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0"
  },
  "engines": {
    "node": "18"
  }
}
